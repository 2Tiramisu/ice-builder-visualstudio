<?xml version="1.0" encoding="utf-8" ?>
<Project ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <PropertyGroup>
        <TimeStampServer>http://timestamp.verisign.com/scripts/timstamp.dll</TimeStampServer>
    	<SignToolPath Condition="Exists('$(WindowsSDK80Path)bin\x86\signtool.exe')">$(WindowsSDK80Path)bin\x86\signtool.exe</SignToolPath>
    	<SignCommand Condition="Exists('$(CertFile)')">"$(SignToolPath)" sign /f "$(CertFile)" /p $(CertPassword) /t $(TimeStampServer)</SignCommand>
    </PropertyGroup>
</Project>
